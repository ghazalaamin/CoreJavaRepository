package collection_program1;

import java.util.Comparator;
import java.util.HashSet;
import java.util.List;
import java.util.Map;
import java.util.Set;
import java.util.stream.Collector;
import java.util.stream.Collectors;
import java.util.stream.Stream;

public class ZzzzFindTheMinIDEmplyeeTest12 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		EmployeeHandlingOperation emp = new EmployeeHandlingOperation();
		List<EmployeeEnity> data = emp.getEmployee(); 
	       
	     //12.FIND THE EMPLYEE HAVING SMALLEST ID 
	
	      Object EmployeeEnity = data.stream().collect(Collectors.minBy(Comparator.comparing(EmployeeEnity::empId())))
	      System.out.println(EmployeeEnity);
	}

}
